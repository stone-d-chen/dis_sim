	publisher = {OUP Oxford},
	author = {Pawitan, Yudi},
	month = jun,
	year = {2001},
	keywords = {Business \& Economics / Econometrics, Mathematics / Probability \& Statistics / General, Reference / General}
}

@book{kruschke_doing_2014,
